<!DOCTYPE html>
<html class="nice-scroll">
	<head>
		<title>{{deckname}}</title>
	</head>
	<body>
		<div id="header">

		</div>
		<div id="deckview">

		</div>
	</body>
	<script>
		document.addEventListener("DOMContentLoaded", function(event) { 
			WS.event.emit('deckview.load', { el: '#deckview', deckid: '{{deckid}}', loggedin: '{{loggedin}}', userid: '{{userid}}' })
		})
	</script>
	{{> header}}
</html>